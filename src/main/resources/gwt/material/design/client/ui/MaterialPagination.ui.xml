<!DOCTYPE ui:UiBinder SYSTEM "http://dl.google.com/gwt/DTD/xhtml.ent">
<ui:UiBinder xmlns:ui="urn:ui:com.google.gwt.uibinder"
	xmlns:g="urn:import:com.google.gwt.user.client.ui"
	xmlns:m="urn:import:gwt.material.design.client.ui"
	xmlns:c="urn:import:gwt.material.design.client.custom">
	<ui:style>
		.name{
			width: 60px;
			font-size: 0.9em;
			padding-top: 5px;
		}
	</ui:style>
	<m:UnorderedList ui:field="pager" addStyleNames="pagination">
		<m:ListItem><m:MaterialLink ui:field="prev" icon="mdi-navigation-chevron-left"/></m:ListItem>
		<m:ListItem><m:MaterialLink ui:field="lblName" addStyleNames="{style.name}"/></m:ListItem>
		<m:ListItem><m:MaterialLink ui:field="next" icon="mdi-navigation-chevron-right"/></m:ListItem>
	</m:UnorderedList>
</ui:UiBinder> 